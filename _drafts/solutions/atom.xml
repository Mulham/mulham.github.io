---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">

	<title>Mulham Solutions</title>
	<link href="http://mulham.github.io/solutions/atom.xml" rel="self"/>
	<link href="http://mulham.github.io/solutions"/>
	<updated>{{ site.time | date_to_xmlschema }}</updated>
	<id>http://mulham.github.io/solutions</id>
	<author>
		<name>Mulham</name>
		<email>mulham94@gmail.com</email>
	</author>

	{% for post in site.posts %}
		<entry>
			<title>{{ post.title }}</title>
			<link href="http://mulham.github.io{{ post.url }}"/>
			<updated>{{ post.date | date_to_xmlschema }}</updated>
			<id>http://mulham.github.io/{{ post.id }}</id>
			<content type="html">{{ post.content | xml_escape }}</content>
		</entry>
	{% endfor %}

</feed>
